{
    "server_config" : {
        "host" : "0.0.0.0",
        "port" : 880,
        "tls_config": {
            "enabled": false
        }
    },
    "access_config" : {
        "backend_path": "yourbackendPath",
        "access_token": "yourAccessToken"
